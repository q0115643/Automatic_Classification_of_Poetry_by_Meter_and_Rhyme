<TEI xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.tei-c.org/ns/1.0" version="5.0">
        <teiHeader type="text">
            <fileDesc>
                <titleStmt>
                    <title>The Kraken</title>
                    <author>Alfred Lord Tennyson</author>
                </titleStmt>
                <publicationStmt><date>1830</date></publicationStmt>
                <sourceDesc default="false"><p/></sourceDesc>
            </fileDesc>
